{
    "extends": "next/core-web-vitals",
    "plugins": ["import", "unused-imports"],
    "rules": {
        "import/order": [
            "error",
            {
                "alphabetize": {
                    "order": "asc", // 昇順にソート
                    "caseInsensitive": true // 小文字大文字を区別する
                }
            }
        ]
    }
}
